{
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index/index.html"
    },
    {
      "source": "/login",
      "destination": "/login/login.html"
    },
    {
      "source": "/menu",
      "destination": "/menu/menu.html"
    },
    {
      "source": "/order",
      "destination": "/order/order.html"
    },
    {
      "source": "/payment",
      "destination": "/payment/payment.html"
    },
    {
      "source": "/profil",
      "destination": "/profil/profile.html"
    },
    {
      "source": "/admin",
      "destination": "/admin/admin.html"
    },
    {
      "source": "/index.js",
      "destination": "/index/index.js"
    },
    {
      "source": "/index.css",
      "destination": "/index/index.css"
    },
    {
      "source": "/login.js",
      "destination": "/login/login.js"
    },
    {
      "source": "/login.css",
      "destination": "/login/login.css"
    },
    {
      "source": "/menu.js",
      "destination": "/menu/menu.js"
    },
    {
      "source": "/menu.css",
      "destination": "/menu/menu.css"
    },
    {
      "source": "/order.js",
      "destination": "/order/order.js"
    },
    {
      "source": "/order.css",
      "destination": "/order/order.css"
    },
    {
      "source": "/payment.js",
      "destination": "/payment/payment.js"
    },
    {
      "source": "/payment.css",
      "destination": "/payment/payment.css"
    },
    {
      "source": "/profile.js",
      "destination": "/profil/profile.js"
    },
    {
      "source": "/profile.css",
      "destination": "/profil/profile.css"
    },
    {
      "source": "/admin.js",
      "destination": "/admin/admin.js"
    },
    {
      "source": "/admin.css",
      "destination": "/admin/admin.css"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
} 